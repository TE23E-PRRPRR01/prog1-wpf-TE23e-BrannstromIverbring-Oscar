<Window x:Class="AIShupUp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIShupUp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" SizeToContent="WidthAndHeight">
    <StackPanel Margin="10">
        <Label FontSize="20" FontWeight="Bold">AI Chatt App</Label>

        <TextBox Name="txbPrompt" Height="100" Margin="10" Padding="10" Width="400"></TextBox>

        <Button Click="KlickSkicka" Margin="10" Padding="10">Skicka</Button>

        <TextBox IsReadOnly="True" Name="txbSvar" Height="100" Margin="10" Padding="10" Width="400"></TextBox>
    </StackPanel>
</Window>
