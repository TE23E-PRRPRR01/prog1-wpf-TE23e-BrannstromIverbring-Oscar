<Window x:Class="SlumpaApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SlumpaApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#EEE"
        SizeToContent="WidthAndHeight">
    <StackPanel>
    <TextBox></TextBox>
    <Label Margin="10" FontSize="18">Slumpa ett tal 1-100</Label>
    <Button Click="KlickSlumpa" Margin="10" Padding="10">Slumpa!</Button>
    <TextBox Name="txbResultat" Margin="10" Padding="10" Width="200" Height="200" FontSize="100"> </TextBox>
    </StackPanel>
</Window>